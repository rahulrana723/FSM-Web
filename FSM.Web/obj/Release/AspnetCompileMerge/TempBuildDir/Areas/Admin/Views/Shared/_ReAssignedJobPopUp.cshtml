@model  FSM.Web.Areas.Employee.ViewModels.JobsViewModel
<div class="modal fade" id="modalReAssignedJobPopUp" role="dialog">
    <div class="modal-dialog">
        <div class="modal-content" style="width:61%;margin-left:23%;">
            <div class="modal-header">
                <button type="button" id="btncross" class="close closebtn" data-dismiss="modal">close</button>

                <h4 class="modal-title">Re-Assigned Job</h4>
            </div>
            <div class="assign-job-message" style="display:none;margin-left: 10px;"></div>
                <div id="ErrorDv" style="display:none;color:red;padding: 5px 0px 0px 18px;"></div>
                <div class="modal-body" style="height:130px">

                    @Html.HiddenFor(model => model.Id, new { @id = "jobIdCal" })
                    @Html.HiddenFor(model => model.AssignTo, new { @id = "assignIdCal" })
                    <div class="form-group">
                        @Html.LabelFor(model => model.DateBooked)
                        @Html.EditorFor(model => model.DateBooked, new { htmlAttributes = new { @id = "DateBookedCalender", @class = "form-control" } })
                    </div>
                    <div class="form-group">
                        @Html.LabelFor(model => model.AssignTo)
                        @Html.DropDownListFor(m => m.AssignTo, Model.OTRWList, "Select OTRW User", new { @id = "tempAssignTo", @title = "Select OTRW User", @class = "ddljobslist form-control Create_Customer" })
                    </div>
                </div>

                <div class="modal-footer">
                    <div class="form-group">
                        <div class="msgsend">
                            <div class="chatBtnsDv">
                                <input type="button" value="Update" id="UpdateReAssignJob" class="btn btn-success chatBtns" />
                            </div>
                        </div>
                    </div>
                </div>
            </div>
    </div>
</div>
<script>
    $(document).ready(function () {
        $('#DateBookedCalender').datepicker({ dateFormat: 'dd M yy' });
    });
</script>


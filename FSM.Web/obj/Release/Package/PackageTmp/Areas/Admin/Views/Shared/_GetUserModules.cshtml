@model FSM.Web.Areas.Admin.ViewModels.ManageRolesViewModel

<div class="user_modules">
    <b>User Modules</b> 
</div>


@for (int i = 0; i < Model.ModulesList.Count(); i++)
{
    @Html.CheckBoxFor(m => Model.ModulesList[i].IsSelected, new { @class = "rolemodule gap_module", data_modulename = Model.ModulesList[i].ModuleName })
    @Html.DisplayFor(m => Model.ModulesList[i].ModuleName)
    @Html.HiddenFor(m => Model.ModulesList[i].Id)
    <br />
}



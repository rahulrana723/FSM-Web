@model  List<FSM.Web.Areas.Employee.ViewModels.SalesBonusViewModel>
@using GridMvc.Html

@{
    Layout = null;
}
<html>
<head>
    @*<script src="~/Scripts/jquery-1.12.4.js"></script>
        <script src="~/Scripts/bootstrap.min.js"></script>
        <meta name="viewport" content="width=device-width" />
        <title>Export User TimeSheet</title>
        <link rel="stylesheet" href="@Url.Content("~/Content/fonts/font-awesome.min.css")">
        <link rel="stylesheet" href="@Url.Content("~/Content/css/style.css")">
        <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
        <link href="~/Content/Site.css" rel="stylesheet" />
            <link rel="shortcut icon" href="@Url.Content("~/Content/images/fsmfavicon.ico")" />*@
    <link href="~/Content/css/font-awesome.min.css" rel="stylesheet" />
    <style>
        ul, p, h1, h2, h3, h4, h5, h6 {
            margin: 0;
            padding: 0;
        }

        li {
            list-style: none;
        }

        body {
            font-family: 'Open Sans';
            font-size: 14px;
            line-height: 25px;
            margin: 0;
        }


        .padding0 {
            padding: 0;
        }

        body {
            font-family: 'Open Sans';
            font-size: 14px;
            line-height: 25px;
            margin: 0;
        }

        table, th, td {
            border: 1px solid black;
            border-collapse: collapse;
            vertical-align: top;
        }

        th, td {
            padding: 5px;
        }

        .padding0 {
            padding: 0;
        }
        /********/
        .invoice_template table {
            border: 1px solid;
            width: 100%;
        }

        .text-right {
            text-align: right;
        }

        .text-center {
            text-align: center;
        }

        .payment_tab {
            margin-bottom: 20px;
        }

        .text-right td, .text-right table {
            border: 0 none;
        }

        td.border {
            border-left: 1px solid;
            padding: 20px;
        }

        .border_btm {
            border-bottom: 1px solid;
        }

        .balance {
            padding: 27px 20px;
        }

        .price_tab {
            padding: 31px 20px;
        }

        .sale_tab {
            padding-top: 36px;
        }
        /**** job time report ****/
        .report_left {
            float: left;
            padding: 20px;
            width: 46%;
        }

        .report_heading, .job_time_report, .report_template {
            float: left;
            width: 100%;
        }

        .job_head {
            border-top: 1px solid #000000;
            float: left;
            text-align: center;
            width: 100%;
        }

            .job_head h2 {
                float: left;
                font-size: 30px;
                margin: 0;
                padding: 30px 0;
                width: 100%;
            }

        .report_template table {
            width: 100%;
        }

        .report_left:last-child {
            border-left: 1px solid #cccccc;
            padding-bottom: 36px;
            text-align: right;
            float: right;
            width: 44%;
        }

        .report_left h4 {
            font-size: 18px;
            padding-bottom: 5px;
        }
    </style>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>
</head>
<body>
    <div class="job_time_report">
        <div class="report_heading">
            <div class="report_left">
                <a href="@Url.Action("Dashboard","Dashboard", new {@area="Admin" })"> <img alt="" src="~/Content/images/srg_logo.png"></a>

            </div><!--report_left-->
            <div class="report_left">
                <h4>OPERATIONS</h4>
                <p><strong>Ph: </strong> 0280205777 </p>
                <p><strong>Email:</strong> Info@sydneyroofandgutter.com.au </p>
                <h4>ACCOUNTS</h4>
                <p><strong>Ph:</strong> 0424032965</p>
                <p><strong>Email:</strong> accounts@sydneyroofandgutter.com.au </p>
            </div><!--report_left-->

        </div><!--report_heading-->
        <div class="job_head">
            <h2> SALES BONUS REPORT </h2>

        </div>
        <div>   Date: @DateTime.Now.Date.ToString("yyyy-MM-dd")</div>
        @if (Model.Count() > 0)
            {
            <div>(@Model.FirstOrDefault().StartDate - @Model.FirstOrDefault().EndDate)</div>}

        <div class="report_template">
            <div class="table-responsive">
                @if (Model.Count() > 0)
            {
                <table class="table text-center">
                    <thead>
                        <tr>
                            <th>Date</th>
                            <th>OTRW Name</th>
                            <th>CO LAst Name</th>
                            <th> Address</th>
                            <th>Inv No.</th>
                            <th>Sale Income</th>
                            <th>JSPO Costs</th>
                            <th>StockItemCost</th>
                            <th>Sales Bonus Costs</th>
                            <th>TotalCost</th>
                            <th>Labour Only Income</th>
                            <th>Labour Cost</th>
                            <th>Labour Profit</th>
                            <th>REVT</th>
                            <th>NRT</th>
                            <th>Time</th>
                            <th>LI/HR</th>
                            <th>LC/HR</th>
                            <th>LP/HR</th>
                            
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var val in Model)
                            {
                            <tr class="success" style="page-break-inside: avoid;">

                                <td>
                                    @if (val.InvoiceDate.HasValue)
                                        {
                                        <p>@String.Format("{0:yyyy-MM-dd}", val.InvoiceDate)</p>
                                        @*<p>@val.InvoiceDate </p>*@
                                        }
                                </td>
                                <td>
                                    <p>@val.OTRWName</p>

                                </td>
                                <td>


                                    <p>@val.CustomerLastName  </p>

                                </td>
                                <td>
                                    <p>@val.SiteFileName  </p>

                                </td>
                                <td>


                                    <p>@val.InvoiceNo  </p>

                                </td>
                                <td>
                                    @if (val.SaleIncome.HasValue)
                                        {
                                        <p>@val.SaleIncome</p>
                                        }
                                        else
                                        {<p>@("0.0")</p>}
                                </td>
                                <td>
                                    @if (val.JSPOCost.HasValue)
                                        {
                                        <p>@val.JSPOCost</p>
                                        }
                                        else
                                        { <p>@("0.0")</p>}

                                </td>
                                <td>
                                    @if (val.StockItemCost.HasValue)
                                        {
                                        <p>@val.StockItemCost</p>
                                        }
                                        else
                                        { <p>@("0.0")</p>}

                                </td>

                                <td>
                                    @if (val.SalesBonus.HasValue)
                                        {
                                        <p>@val.SalesBonus</p>
                                        }
                                        else
                                        { <p>@("0.0")</p>}

                                </td>
                                <td>

                                    @if (val.TotalCost.HasValue)
                                        {
                                        <p>@val.TotalCost</p>
                                        }
                                        else
                                        { <p>@("0.0")</p>}

                                </td>
                                <td>

                                    @if (val.LabourIncome.HasValue)
                                        {
                                        <p>@val.LabourIncome</p>
                                        }
                                        else
                                        { <p>@("0.0")</p>}

                                </td>
                                <td>

                                    @if (val.LabourCost.HasValue)
                                        {
                                        <p>@val.LabourCost</p>
                                        }
                                        else
                                        { <p>@("0.0")</p>}

                                </td>
                                <td>

                                    @if (val.LabourProfit.HasValue)
                                        {
                                        <p>@val.LabourProfit </p>
                                        }
                                        else
                                        { <p>@("0.0")</p>}

                                </td>
                                <td>



                                    <p>@val.Rev_Hours </p>



                                </td>
                                <td>


                                    <p>@val.NRL_Hours </p>


                                </td>
                                <td>



                                    <p>@val.Labour_Hours </p>


                                </td>


                                <td>
                                    @if (val.LobourIncomePerHour.HasValue)
                                        {
                                        <p>@val.LobourIncomePerHour</p>
                                        }
                                        else
                                        { <p>@("0.0")</p>}





                                </td>
                                <td>
                                    @if (val.LabourCostPerHour.HasValue)
                                        {
                                        <p>@val.LabourCostPerHour</p>
                                        }
                                        else
                                        { <p>@("0.0")</p>}





                                </td>
                                <td>
                                    @if (val.LabourProfitPerHour.HasValue)
                                        {
                                        <p>@val.LabourProfitPerHour</p>
                                        }
                                        else
                                        { <p>@("0.0")</p>}




                                </td>
                              

                            </tr>
                            }
                    </tbody>
                </table>
                }
                else
                {
                <div class="alert alert-danger">
                    <strong>!</strong>No Record found !
                </div>
                }
            </div>
        </div>
    </div>
</body>
</html>
